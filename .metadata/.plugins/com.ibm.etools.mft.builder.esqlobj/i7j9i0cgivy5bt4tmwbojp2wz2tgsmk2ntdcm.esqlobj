CREATE COMPUTE MODULE MF_MAIN_Route_Message
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		
		CREATE FIELD Environment.Variables;
		CREATE LASTCHILD OF Environment.Variables DOMAIN 'JSON';
		CREATE FIELD Environment.Variables.JSON.Data IDENTITY (JSON.Object)Data;
		
		SET Environment.Variables.JSON.Data = SESSION.Data[1];
		
		SET OutputRoot.BLOB.BLOB = CAST(ASBITSTREAM(Environment.Variables.SESSION CCSID 1208) AS BLOB CCSID 819);
	END;
END MODULE;