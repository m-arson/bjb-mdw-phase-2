CREATE COMPUTE MODULE MF_MAIN_Generate_and_Distribute_Echo
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		
		IF SESSION.Field IS NOT NULL THEN
			DECLARE I, J INTEGER;
			SET I = 1;
			SET J = CARDINALITY(Session.*[]);
			
			WHILE I <= J DO
				
				SET I = I + 1;
			END WHILE;
			
		END IF;
		
		RETURN TRUE;
	END;
END MODULE;