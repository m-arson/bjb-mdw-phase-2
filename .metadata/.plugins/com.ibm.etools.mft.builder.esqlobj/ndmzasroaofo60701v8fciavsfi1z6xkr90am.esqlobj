	CREATE FUNCTION GetSessionLength(IN S REFERENCE) RETURNS INTEGER
	BEGIN
		IF S = NULL THEN
			RETURN 0;
		ELSE
			DECLARE L INTEGER;
			SET L = CARDINALITY(S.Data[]);
			RETURN L;	
		END IF;
	END;
	
	CREATE PROCEDURE GetSessionIndex(IN H CHARACTER, IN S REFERENCE, IN B REFERENCE)
	BEGIN
		DECLARE I, L INTEGER;
		SET I = 1;
		SET L = GetSessionLength(S);
		
		IF L = 0 THEN
			SET B.Index = 1;
			SET B.Avail = FALSE;
		ELSE
			WHILE I <= L DO
				IF S.Data[I].Address = H THEN
					SET B.Index = I;
					SET B.Avail = TRUE;
				END IF;
				SET I = I + 1;
			END WHILE;
		END IF;
		SET B.Index = I;
		SET B.Avail = FALSE;
	END;
	
