/*EATE FUNCTION Main() RETURNS BOOLE*/
	BEGIN
		
		CREATE LASTCHILD OF OutputRoot DOMAIN 'BLOB' NAME 'BLOB';
		SET OutputRoot.BLOB.BLOB = ASBITSTREAM(InputRoot.JSON.Data CCSID 1208);
		
		DECLARE ts CHARACTER CAST(CURRENT_TIMESTAMP AS CHARACTER FORMAT 'yyyyMMdd_HHmmssSSSSSS');
		SET OutputLocalEnvironment.Destination.File.Name = 'POSTING_LOG_' || ts;
		
		RETURN TRUE;
	END;
